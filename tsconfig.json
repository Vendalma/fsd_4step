{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "strict": true,
    "noImplicitReturns": false,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true
  },
  "include": [
    "./src/**/*"
  ]
}